{
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 30
    }
  },
  "images": {
    "formats": ["image/webp"],
    "minimumCacheTTL": 86400,
    "sizes": [16, 32, 48, 64, 96, 128, 256, 384]
  },
  "git": {
    "lfs": false
  }
} 